# Production environment variables
NEXT_PUBLIC_APP_ENV=production

# API configuration
NEXT_PUBLIC_API_URL=/api
NEXT_PUBLIC_API_TIMEOUT=30000

# Google Gemini API Key (replace with actual key in deployment environment)
# Format: Your Google Gemini API key from https://makersuite.google.com/app/apikey
GOOGLE_GENAI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Analytics (replace with actual IDs in deployment environment)
# Format: UA-XXXXXXXXX-X or G-XXXXXXXXXX
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Error tracking (replace with actual DSN in deployment environment)
# Format: https://XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX@XXXX.ingest.sentry.io/XXXXXXX
NEXT_PUBLIC_SENTRY_DSN=https://examplePublicKey@XXXX.ingest.sentry.io/exampleProjectId

# Debug settings (disabled in production)
NEXT_PUBLIC_ENABLE_DEBUG=false
NEXT_PUBLIC_ENABLE_LOGGING=false

# Game specific settings
NEXT_PUBLIC_DEFAULT_GAME_TIMER=600
NEXT_PUBLIC_ENABLE_SOUND=true
NEXT_PUBLIC_ENABLE_ANIMATIONS=true

# Feature flags
NEXT_PUBLIC_ENABLE_MULTIPLAYER=true
NEXT_PUBLIC_ENABLE_AI_OPPONENT=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=true

# Cache and optimization settings
NEXT_PUBLIC_CACHE_MAX_AGE=3600
NEXT_PUBLIC_STATIC_CACHE_MAX_AGE=31536000

# Build information (automatically set during build)
NEXT_PUBLIC_BUILD_TIME=2025-05-19T05:46:59Z
NEXT_PUBLIC_VERSION=1.0.0
